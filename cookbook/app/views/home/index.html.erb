<h1>Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>

<% if user_signed_in? %>
  <p>Hello <%= current_user.full_name %></p>
  <p><%= link_to "Edit your profile", edit_user_registration_path %></p>
  <p><%= link_to "Log out", destroy_user_session_path, method: :DELETE %></p>
<% else %>
  <p><%= link_to "Sign in", new_user_session_path %></p>
  <p><%= link_to "Sign up", new_user_registration_path %></p>
<% end %>

